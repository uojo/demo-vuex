    "vue-jest": "^2.6.0",
    "vue-template-compiler": "^2.5.16",
    "vuex": "^3.0.1"
  },
  "jest": {
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
